export
fn test_vpermd(reg u64 tmp)
{
  reg u256 t0 t1;

  t0 = #set0_256();
  t1 = #set0_256();

  t0 = #VPERMD(t0, t1);
  t0 = #VPERMD(t0, [:u256 tmp]);
  [:u256 tmp] = t0;

  reg u512 tt0 tt1;

  tt0 = #set0_512();
  tt1 = #set0_512();

  tt0 = #VPERMD_512(tt0, tt1);
  tt0 = #VPERMD_512(tt0, [:u512 tmp]);
  [:u512 tmp] = tt0;
}
